{
  "name": "has-banner",
  "description": "Returns true if a file has a banner code comment. Used for linting.",
  "version": "0.2.0",
  "homepage": "https://github.com/jonschlinkert/has-banner",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/has-banner",
  "bugs": {
    "url": "https://github.com/jonschlinkert/has-banner/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "strip-bom-string": "^0.1.2",
    "strip-use-strict": "^0.1.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.7",
    "mocha": "^2.4.5"
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "strip-banner",
        "add-banner",
        "write-banner",
        "update-banner"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
